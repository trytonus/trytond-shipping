<?xml version="1.0"?>
<data>
    <xpath expr="/form/field[@name='shipment']" position="after">
        <notebook colspan="4">
            <page id="weight" string="Weight Details">
                <label name="computed_weight"/>
                <field name="computed_weight"/>
                <label name="weight"/>
                <field name="weight"/>
                <label name="weight_uom"/>
                <field name="weight_uom"/>
                <label name="override_weight"/>
                <field name="override_weight"/>
                <label name="tracking_number"/>
                <field name="tracking_number"/>
            </page>
            <page id="dimensions" string="Dimensions">
                <label name="length"/>
                <field name="length"/>
                <label name="width"/>
                <field name="width"/>
                <label name="height"/>
                <field name="height"/>
                <label name="distance_unit"/>
                <field name="distance_unit"/>
                <label name="box_type"/>
                <field name="box_type" widget="selection"/>
            </page>
        </notebook>
    </xpath>
</data>
