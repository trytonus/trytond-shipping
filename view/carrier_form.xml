<data>
    <xpath expr="//field[@name='party']" position="after">
        <label name="active"/>
        <field name="active"/>
    </xpath>
    <xpath expr="/form/field[@name='carrier_cost_method']" position="after">
        <notebook colspan="4">
            <page id="services" string="Services">
                <field name="services" colspan="4"/>
            </page>
            <page id="box_types" string="Box Types">
                <field name="box_types" colspan="4"/>
            </page>
        </notebook>
    </xpath>
</data>
